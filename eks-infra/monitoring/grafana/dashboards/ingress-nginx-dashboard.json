{
    "dashboard": {
      "title": "Ingress NGINX / ALB Dashboard",
      "panels": [
        {
          "title": "Requests Rate",
          "type": "timeseries",
          "targets": [
            {
              "expr": "rate(nginx_ingress_controller_requests[5m])",
              "legendFormat": "{{namespace}}/{{service}} {{status}}"
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
        },
        {
          "title": "Request Latency",
          "type": "timeseries",
          "targets": [
            {
              "expr": "histogram_quantile(0.95, rate(nginx_ingress_controller_request_duration_seconds_bucket[5m]))",
              "legendFormat": "P95"
            }
          ],
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
        }
      ]
    }
  }
  